    <script src="/assets/js/main.js"></script>
    <environment include="Development">
        <script>const isDebug = true;</script>
        <script src="~/developer/js/App/web.development.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script>const isDebug = false;</script>
        <script src="~/developer/js/App/web.production.js" asp-append-version="true"></script>
    </environment>
    <script src="~/developer/js/App/auth-helper.js" asp-append-version="true"></script>
    <script src="~/developer/js/App/jwt-decode.min.js" asp-append-version="true"></script>
    <script src="~/developer/js/App/app.js" asp-append-version="true"></script>
    <script src="~/developer/js/App/site.js" asp-append-version="true"></script>
    <!-- default settings for datatable -->
    @if (requestCultureFeature != "ar")
    {
        <link rel="stylesheet" href="~/developer/css/developer.css" asp-append-version="true" />
        <script src="~/developer/js/App/default-datatable.js" asp-append-version="true"></script>
    }
    else
    {
        <link rel="stylesheet" href="~/developer/css/developer-rtl.css" asp-append-version="true" />
        <script src="~/developer/js/App/default-datatable-ar.js" asp-append-version="true"></script>
    }
    <script src="~/developer/js/App/default-datatable-extra.js" asp-append-version="true"></script>
