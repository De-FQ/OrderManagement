    <link rel="stylesheet" href="/assets/vendor/css/dataTables.bootstrap5.css" />
    <link rel="stylesheet" href="/assets/vendor/css/responsive.bootstrap5.min.css" />
    <div class="toast-container" style="z-index:1000; position: absolute;"></div>
    @{
        await Html.RenderPartialAsync("_ResoursesModalPartial"); 
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    <script>
        $(function () {  setInterval(showDateTime, 1000); }); 
    </script>

    <script type="text/javascript" src="https://cdn.jsdelivr.net/momentjs/latest/moment.min.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.min.js"></script>
</body>
</html>
